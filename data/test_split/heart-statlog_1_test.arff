@ATTRIBUTE exercise_induced_angina REAL
@ATTRIBUTE oldpeak REAL
@ATTRIBUTE slope REAL
@ATTRIBUTE number_of_major_vessels REAL
@ATTRIBUTE thal REAL
@ATTRIBUTE class {absent, present}

